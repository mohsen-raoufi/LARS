      <memberdef kind="function" id="class_q_c_p_axis_ticker_fixed_1a4bc83d85a4f81d4abdd3fa5042d7b833" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void QCPAxisTickerFixed::setTickStep</definition>
        <argsstring>(double step)</argsstring>
        <name>setTickStep</name>
        <qualifiedname>QCPAxisTickerFixed::setTickStep</qualifiedname>
        <param>
          <type>double</type>
          <declname>step</declname>
